@startuml
hide circle

class Bestelling <<Entity>> {
    bestellingId
    budget
    berekenTotaalPrijs()
}

class BestelRegel <<Entity>> {
    productAantal
    productPrijs
}

class Product <<Entity>> {
    productId
    prijs
}

Bestelling "1" -r- "*" BestelRegel
BestelRegel "*" -r- "1" Product

@enduml